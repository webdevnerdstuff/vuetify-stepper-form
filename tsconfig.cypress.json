{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"types": [
			"cypress"
		],
		"typeRoots": [
			"./cypress/types/",
			"node_modules/@types"
		],
		"paths": {
			"@/*": [
				"src/*"
			],
			"@cypress/*": [
				"./cypress/*"
			]
		}
	},
	"include": [
		"src/**/*.cy.ts",
		"src/**/*.cy.tsx",
		"cypress/**/*.ts"
	]
}
